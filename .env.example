# Server Configuration
SERVER_PORT=8091

# Database Configuration
SPRING_DATASOURCE_URL=jdbc:h2:file:./data/leafshotdb;DB_CLOSE_ON_EXIT=FALSE;AUTO_RECONNECT=TRUE
SPRING_DATASOURCE_USERNAME=sa
SPRING_DATASOURCE_PASSWORD=

# Custom LeafShot Configuration
LEAFSHOT_WORKING_DIRECTORY=workdir
LEAFSHOT_UPLOAD_MAX_SIZE_MB=100
LEAFSHOT_RESOURCE_LIFETIME_HOURS=168
LEAFSHOT_RESOURCE_PROLONGABLE=true
LEAFSHOT_RESOURCE_PROLONGED_HOURS_PER_ACCESS=24
LEAFSHOT_REPORTS_DELETION_THRESHOLD=5
LEAFSHOT_REPORTS_TIME_WITHDRAW_HOURS=24
LEAFSHOT_RATE_LIMIT_ENABLED=true
LEAFSHOT_RATE_LIMIT_REQUESTS_PER_MINUTE=10

# Spring Servlet Configuration
SPRING_SERVLET_MULTIPART_MAX_FILE_SIZE=10MB
SPRING_SERVLET_MULTIPART_MAX_REQUEST_SIZE=10MB
